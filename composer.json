{
    "name": "akira/filament-invoice",
    "description": "Filament Invoice",
    "keywords": [
        "akira",
        "laravel",
        "filament-invoice"
    ],
    "homepage": "https://github.com/akira/filament-invoice",
    "support": {
        "issues": "https://github.com/akira/filament-invoice/issues",
        "source": "https://github.com/akira/filament-invoice"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "kidiatoliny",
            "email": "kidiatoliny@akira-io.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Akira\\FilamentInvoice\\": "src/",
            "Akira\\FilamentInvoice\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akira\\FilamentInvoice\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akira\\FilamentInvoice\\FilamentInvoiceServiceProvider"
            ],
            "aliases": {
                "FilamentInvoice": "Akira\\FilamentInvoice\\Facades\\FilamentInvoice"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}